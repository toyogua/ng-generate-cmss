{
  "$schema": "http://json-schema.org/schema",
  "title": "Init Options Schema",
  "type": "object",
  "description": "Options for init schematic",
  "properties": {
    "name": {
      "type": "string",
      "description": "Name of the file",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "Enter name to file?"
    }
  },
  "required": [
    "name"
  ]
}
